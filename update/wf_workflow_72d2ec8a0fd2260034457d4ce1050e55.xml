        <sys_updated_by>Courtney.Richards@weather.com</sys_updated_by>
        <sys_updated_on>2016-10-20 17:56:41</sys_updated_on>
        <value>// Script reads the current Communication Center record to determine&#13;
// the correct time that the notification should go out,&#13;
// then calculates the number of seconds to wait before the next step is to occur.&#13;
// If the scheduled time is not in the future, the notification goes out immediately.&#13;
&#13;
var gdt1 = new GlideDateTime();		// now&#13;
var gdt2 = new GlideDateTime( current.u_scheduled_time );	// time scheduled to send&#13;
var dur = GlideDateTime.subtract(gdt1, gdt2);	// duration until scheduled time&#13;
var nsec = dur.getNumericValue() / 1000;	// # seconds until scheduled time&#13;
var wait = gs.getProperty('twc.wait.brule');    // # seconds to wait for CC business rules to complete&#13;
&#13;
gs.debug("CC Workflow: gdt1="+gdt1+", gdt2="+gdt2+", nsec="+nsec);&#13;
&#13;
var msg = "Notification "+current.number+" is scheduled to be sent "&#13;
        + ( (nsec &lt;= wait) ? "immediately" : "at "+ gdt2.getDisplayValue() )&#13;
;&#13;
&#13;
gs.addInfoMessage(msg);&#13;
&#13;
gs.debug("CC Workflow: answer="+((nsec &lt;= wait) ? wait : nsec));&#13;
answer = (nsec &lt;= wait) ? wait : nsec;&#13;
</value>
        <variable display_value="">293493710a0a0b2115e3e5b4fce1d12a</variable>
    </sys_variable_value>
